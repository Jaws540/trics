{
    "name": "character-instruction-script",
    "displayName": "Character Instruction Script",
    "description": "Syntax highlighting for CIS for Visual Studio Code",
    "version": "0.0.2",
    "icon": "images/cis_icon.png",
    "publisher": "QuantumRetrofit",
    "engines": {
        "vscode": "^1.48.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "cis",
            "aliases": ["Character Instruction Script", "rpgs"],
            "extensions": [".rpgs"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "cis",
            "scopeName": "source.rpgs",
            "path": "./syntaxes/cis.tmLanguage.json"
        }]
    }
}